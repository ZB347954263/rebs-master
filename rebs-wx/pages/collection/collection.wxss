/* pages/collection/collection.wxss */
page{
  background-color:#f0f0f0;
}

.collection-all{
    display: flex;
    padding: 25rpx;
    align-items: center;
    flex-direction: row;
    background-color:#fff;
    margin-top: 25rpx;
    margin-bottom: 25rpx;
}
.collection{
    display: flex;
    padding: 25rpx;
    align-items: center;
    flex-direction: row;
    border-bottom: 1rpx solid #f0f0f0;
    background-color:#fff;
}

.collection-item{
    flex: 1;
    font-size: 30rpx;
    padding-left:15rpx;
}

.collection-arrow{
    width: 32rpx;
    height: 32rpx;
    color: green;
    font-size: 30rpx;
    padding-left:10rpx;
}

.icon-label-class{
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  margin-left: 4rpx;
  background: url("data:image/png;base64,")no-repeat center/10rpx 18rpx;
}
